<?xml version="1.0" encoding="UTF-8"?>
<database>
    <element label="Question" max_length="40" name="x_sl_question" type="collection">
        <element label="Flagged" max_length="40" name="flagged_for_admin_review" type="boolean"/>
        <element label="Guidance" max_length="5000" name="guidance" type="html"/>
        <element label="No Action" max_length="40" name="no_action" type="string"/>
        <element label="No Widget ID" max_length="40" name="no_widget_id" type="string"/>
        <element label="Number" max_length="40" name="number" type="integer"/>
        <element choice="1" default="Both" label="Service Type" mandatory="true" max_length="40" name="service_type" type="string">
            <choice>
                <element label="FS" sequence="0" value="FS"/>
                <element label="CS" sequence="1" value="CS"/>
                <element label="Both" sequence="2" value="Both"/>
            </choice>
        </element>
        <element label="Text" mandatory="true" max_length="1000" name="text" type="string"/>
        <element label="Title" max_length="40" name="title" type="string"/>
        <element label="Yes Action" max_length="40" name="yes_action" type="string"/>
        <element label="Yes Widget ID" max_length="40" name="yes_widget_id" type="string"/>
    </element>
</database>
